"use strict";

var _interopRequireDefault = require("C:/Users/Otomfon/Desktop/Math-Magicians/node_modules/@babel/runtime/helpers/interopRequireDefault.js").default;

var _operate = _interopRequireDefault(require("../logic/operate"));

describe('test for opertae file', () => {
  test('test for addition', () => {
    const one = 4;
    const two = 16;
    const operation = '+';
    const response = (0, _operate.default)(one, two, operation);
    expect(response).toBe('20');
  });
  test('test for subtraction', () => {
    const one = 16;
    const two = 4;
    const operation = '-';
    const response = (0, _operate.default)(one, two, operation);
    expect(response).toBe('12');
  });
  test('test for multiplication', () => {
    const one = 4;
    const two = 4;
    const operation = 'x';
    const response = (0, _operate.default)(one, two, operation);
    expect(response).toBe('16');
  });
  test('test for divition', () => {
    const one = 44;
    const two = 4;
    const operation = 'รท';
    const response = (0, _operate.default)(one, two, operation);
    expect(response).toBe('11');
  });
});
