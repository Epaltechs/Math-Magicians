"use strict";

var _interopRequireDefault = require("C:/Users/Otomfon/Desktop/Math-Magicians/node_modules/@babel/runtime/helpers/interopRequireDefault.js").default;

var _calculate = _interopRequireDefault(require("../logic/calculate"));

describe('Test for the button AC', () => {
  test('it should return Obj with null values', () => {
    const obj = {
      total: 5,
      next: 2,
      operation: '+'
    };
    const btnValue = 'AC';
    const response = (0, _calculate.default)(obj, btnValue);
    expect(response.total).toBe(null);
    expect(response.next).toBe('0');
    expect(response.operation).toBe(null);
  });
});
describe('Test for button with type of number ', () => {
  test('case 1: total and operation are null', () => {
    const obj = {
      total: null,
      next: '0',
      operation: null
    };
    const btnValue = '0';
    const response = (0, _calculate.default)(obj, btnValue);
    expect(response).toEqual({});
  });
});
